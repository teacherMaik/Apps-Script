function checkDni(res) {
  // List Letters
  const letters = "TRWAGMYFPDXBNJZSQVHLCKE";

  // Example: res -> "X6245200X" o "03251853K"
  res = res.trim().toUpperCase();

  // Get Letter at end
  let letter = res.slice(-1);

  // Get numebrs (special conversion for Spanish NIE, X-> 0, Y-> 1, Z-> 2)
  let number = res.slice(0, -1);

  Logger.log(res);
  Logger.log(letter);
  Logger.log(number);
  if (number[0] === "X") {
    number = number.replace("X", "0");
  } else if (number[0] === "Y") {
    number = number.replace("Y", "1");
  } else if (number[0] === "Z") {
    number = number.replace("Z", "2");
  }

  // Convert digits stringn to Int
  number = parseInt(number, 10);

  // Modular Division by 23 to get letter index from list
  let expectedLetter = letters[number % 23];

  // Check
  Logger.log(expectedLetter);
  
  return letter === expectedLetter;
}
